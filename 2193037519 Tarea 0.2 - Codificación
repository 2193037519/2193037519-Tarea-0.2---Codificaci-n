#include<stdio.h>

int main(){    
 int i;
 float Calificaciones[7]={7.7, 8.2,10.0,6.7,9.6,6.7,9.4};
 float objetivo;
    printf("Busqueda lineal \n");
    for(i=0;i<7;i++){
        printf("%.2f ",Calificaciones[i]);
    }
    printf("\n");
    printf("Ingrese el numero buscado: ");
    scanf("%f", &objetivo);

  /* Colocar el codigo aqui para la busqueda lineal de 
  un elemento y que devuelva si encontro el elemento o no*/
  
    for (i=0; i<7; i++){
      if (objetivo == Calificaciones[i]){
        printf ("La posicon del numero buscado es: %i", i);
        break;
      }
  	  if (i == 6){
          printf ("numero no encontrado");
		  }
    }

   return 0;
}
